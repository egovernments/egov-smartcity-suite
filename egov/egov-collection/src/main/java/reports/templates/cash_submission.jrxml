	<queryString>
		<![CDATA[SELECT
     EGCL_COLLECTIONPAYEEDETAILS."PAYEENAME" AS PAYEE_NAME,
     EG_DEPARTMENT."DEPT_NAME" AS DEPT_NAME,
     EG_SERVICEDETAILS."SERVICENAME" AS SERVICE_NAME,
     EGF_INSTRUMENTHEADER."INSTRUMENTAMOUNT" AS INSTRUMENT_AMOUNT,
     EGCL_COLLECTIONHEADER."ID" AS COLLECTIONHEADER_ID,
     BANK."NAME" AS REMIT_BANK_NAME,
     BANKBRANCH."BRANCHNAME" AS REMIT_BRANCH_NAME,
     BANKACCOUNT."ACCOUNTNUMBER" AS REMIT_ACCOUNT_NUM
FROM
     "EGCL_COLLECTIONHEADER" EGCL_COLLECTIONHEADER INNER JOIN "EGCL_COLLECTIONMIS" EGCL_COLLECTIONMIS ON EGCL_COLLECTIONHEADER."ID" = EGCL_COLLECTIONMIS."ID_COLLECTIONHEADER"
     INNER JOIN "EGCL_COLLECTIONPAYEEDETAILS" EGCL_COLLECTIONPAYEEDETAILS ON EGCL_COLLECTIONHEADER."ID_COLLECTIONPAYEEDETAILS" = EGCL_COLLECTIONPAYEEDETAILS."ID"
     INNER JOIN "EG_SERVICEDETAILS" EG_SERVICEDETAILS ON EGCL_COLLECTIONHEADER."ID_SERVICE" = EG_SERVICEDETAILS."ID"
     INNER JOIN "EG_LOCATION" EG_LOCATION ON EGCL_COLLECTIONHEADER."ID_LOCATION" = EG_LOCATION."ID"
     INNER JOIN "EGCL_COLLECTIONINSTRUMENT" EGCL_COLLECTIONINSTRUMENT ON EGCL_COLLECTIONHEADER."ID" = EGCL_COLLECTIONINSTRUMENT."COLLECTIONHEADERID"
     INNER JOIN "EGF_INSTRUMENTHEADER" EGF_INSTRUMENTHEADER ON EGCL_COLLECTIONINSTRUMENT."INSTRUMENTMASTERID" = EGF_INSTRUMENTHEADER."ID"
     INNER JOIN "EGF_INSTRUMENTTYPE" EGF_INSTRUMENTTYPE ON EGF_INSTRUMENTHEADER."INSTRUMENTTYPE" = EGF_INSTRUMENTTYPE."ID"
     INNER JOIN "EG_DEPARTMENT" EG_DEPARTMENT ON EGCL_COLLECTIONMIS."ID_DEPARTMENT" = EG_DEPARTMENT."ID_DEPT"
     INNER JOIN "EG_BOUNDARY" EG_BOUNDARY ON EGCL_COLLECTIONMIS."ID_BOUNDARY" = EG_BOUNDARY."ID_BNDRY"
     LEFT JOIN "CONTRAJOURNALVOUCHER" CONTRAJOURNALVOUCHER ON CONTRAJOURNALVOUCHER."INSTRUMENTHEADERID"=EGF_INSTRUMENTHEADER."ID"
     LEFT JOIN "BANKACCOUNT" BANKACCOUNT ON BANKACCOUNT."ID" = CONTRAJOURNALVOUCHER."TOBANKACCOUNTID"
     LEFT JOIN "BANKBRANCH" BANKBRANCH ON BANKBRANCH."ID" = BANKACCOUNT."BRANCHID"
     LEFT JOIN "BANK" BANK ON BANK."ID" = BANKBRANCH."BANKID"
WHERE
 EGCL_COLLECTIONINSTRUMENT."COLLECTIONHEADERID" = EGCL_COLLECTIONHEADER."ID"
 AND EGF_INSTRUMENTTYPE."TYPE" = $P{EGOV_INSTRUMENT_TYPE}
 AND EGCL_COLLECTIONHEADER."CREATED_DATE" >= $P{EGOV_FROM_DATE}
 AND EGCL_COLLECTIONHEADER."CREATED_DATE" <= $P{EGOV_TO_DATE_PLUS_ONE}
 $P!{Q_COUNTER_ID}
 $P!{Q_COUNTER_OPERATOR_ID}
 $P!{Q_BOUNDARY_ID}
 $P!{Q_RECEIPT_IDS}]]>
	</queryString>

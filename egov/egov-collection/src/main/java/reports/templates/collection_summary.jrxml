	</parameter>
	<parameter name="Q_PAYMENT_MODES" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{EGOV_PAYMENT_MODE} == "cash" ? "AND EGF_INSTRUMENTTYPE.\"TYPE\" = 'cash'" : 
$P{EGOV_PAYMENT_MODE} == "card" ? "AND EGF_INSTRUMENTTYPE.\"TYPE\" = 'card'" : 
$P{EGOV_PAYMENT_MODE} == "bank" ? "AND EGF_INSTRUMENTTYPE.\"TYPE\" = 'bank'" : 
$P{EGOV_PAYMENT_MODE} == "cheque/dd" ? "AND EGF_INSTRUMENTTYPE.\"TYPE\" IN ('cheque', 'dd')" :
"AND EGF_INSTRUMENTTYPE.\"TYPE\" IN ('cash', 'card', 'bank', 'cheque', 'dd')"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     COUNT(*) AS RECEIPT_COUNT,
     EG_DEPARTMENT."DEPT_NAME" AS DEPARTMENT_NAME,
     DECODE(EGF_INSTRUMENTTYPE."TYPE", 'cash', 'cash', 'bank', 'bank', 'card', 'card', 'cheque/dd') AS PAYMENT_MODE,
     EG_LOCATION."NAME" AS COUNTER_NAME,
     EG_EMPLOYEE."EMP_FIRSTNAME" || ' ' || EG_EMPLOYEE."EMP_LASTNAME" AS EMPLOYEE_NAME,
     SUM(EGF_INSTRUMENTHEADER."INSTRUMENTAMOUNT") AS AMOUNT
FROM
     "EGCL_COLLECTIONHEADER" EGCL_COLLECTIONHEADER INNER JOIN "EGCL_COLLECTIONINSTRUMENT" EGCL_COLLECTIONINSTRUMENT ON EGCL_COLLECTIONHEADER."ID" = EGCL_COLLECTIONINSTRUMENT."COLLECTIONHEADERID"
     INNER JOIN "EGF_INSTRUMENTHEADER" EGF_INSTRUMENTHEADER ON EGCL_COLLECTIONINSTRUMENT."INSTRUMENTMASTERID" = EGF_INSTRUMENTHEADER."ID"
     INNER JOIN "EGW_STATUS" EGW_STATUS ON EGF_INSTRUMENTHEADER."ID_STATUS" = EGW_STATUS."ID"
     INNER JOIN "EG_LOCATION" EG_LOCATION ON EGCL_COLLECTIONHEADER."ID_LOCATION" = EG_LOCATION."ID"
     INNER JOIN "EGF_INSTRUMENTTYPE" EGF_INSTRUMENTTYPE ON EGF_INSTRUMENTHEADER."INSTRUMENTTYPE" = EGF_INSTRUMENTTYPE."ID"
     INNER JOIN "EG_USER" EG_USER ON EGCL_COLLECTIONHEADER."CREATED_BY" = EG_USER."ID_USER"
     INNER JOIN "EG_EMPLOYEE" EG_EMPLOYEE ON EG_USER."ID_USER" = EG_EMPLOYEE."ID_USER"
     INNER JOIN "EG_EMP_ASSIGNMENT_PRD" EG_EMP_ASSIGNMENT_PRD ON EG_EMPLOYEE."ID" = EG_EMP_ASSIGNMENT_PRD."ID_EMPLOYEE"
     INNER JOIN "EG_EMP_ASSIGNMENT" EG_EMP_ASSIGNMENT ON EG_EMP_ASSIGNMENT_PRD."ID" = EG_EMP_ASSIGNMENT."ID_EMP_ASSIGN_PRD"
     INNER JOIN "EG_DEPARTMENT" EG_DEPARTMENT ON EG_DEPARTMENT."ID_DEPT" = EG_EMP_ASSIGNMENT."MAIN_DEPT"
WHERE
     EGCL_COLLECTIONHEADER."CREATED_DATE" >= $P{EGOV_FROM_DATE}
     AND EGCL_COLLECTIONHEADER."CREATED_DATE" < $P{EGOV_TO_DATE_PLUS_ONE}
     AND EGW_STATUS."DESCRIPTION" != 'Cancelled'
     $P!{Q_PAYMENT_MODES}
     $P!{Q_DEPT_ID}
GROUP BY
     EG_DEPARTMENT."DEPT_NAME",
     DECODE(EGF_INSTRUMENTTYPE."TYPE", 'cash', 'cash', 'bank', 'bank', 'card', 'card', 'cheque/dd'),
     EG_LOCATION."NAME",
     EG_EMPLOYEE."EMP_FIRSTNAME" || ' ' || EG_EMPLOYEE."EMP_LASTNAME"
ORDER BY
     EG_DEPARTMENT."DEPT_NAME",
     DECODE(EGF_INSTRUMENTTYPE."TYPE", 'cash', 'cash', 'bank', 'bank', 'card', 'card', 'cheque/dd')]]>
	</queryString>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.GradeMaster" table="EGEIS_GRADE_MSTR">
		<id name="id" column="GRADE_ID" type="int">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">EGPIMS_GRADE_MSTR_SEQ</param>
			</generator>
		</id>
		<property name="name" column="GRADE_VALUE" />
		<property name="fromDate" column="START_DATE" />
		<property name="toDate" column="END_DATE" />
		<property name="age" column="AGE" />
		<property name="orderNo" column="ORDER_NO" />
	</class>
</hibernate-mapping>
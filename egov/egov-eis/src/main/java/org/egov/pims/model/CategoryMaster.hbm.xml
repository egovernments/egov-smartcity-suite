<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.CategoryMaster" table="EGEIS_CATEGORY_MSTR">
		
		<id
					name="id"
					column="CATEGORY_ID"
					type="int">
					<generator class="sequence">
						<param name="sequence_name">EGPIMS_CAT_SEQ</param>
					</generator>
		</id>
		
		<property
			name="name"
			column="CATEGORY_NAME"
			
		/>
		<property
							name="fromDate"
							column="START_DATE"
							
				/>
				<property
							name="toDate"
							column="END_DATE"
							
		/>
		<property	name="fileId" column="FILEID"/>
	</class>
</hibernate-mapping>
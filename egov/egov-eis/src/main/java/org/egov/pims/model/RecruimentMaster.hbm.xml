<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.RecruimentMaster" table="EGEIS_MODE_OF_RECRUIMENT_MSTR">
		
		<id
							name="id"
							column="MODE_OF_RECRUIMENT_ID"
							type="int">
							<generator class="sequence">
								<param name="sequence_name">EGPIMS_MODE_OF_RECRUIMENT_SEQ</param>
							</generator>
		</id>
		<property
			name="name"
			column="MODE_OF_RECRUIMENT_NAME"
			
		/>
		<property
					name="fromDate"
					column="START_DATE"
					
		/>
		<property
					name="toDate"
					column="END_DATE"
					
		/>	
	</class>
</hibernate-mapping>
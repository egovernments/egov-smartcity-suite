<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.ReligionMaster" table="EGEIS_RELIGION_MSTR">
		
		<id
							name="id"
							column="RELIGION_ID"
							type="int">
							<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
								<param name="sequence_name">EGPIMS_RELIGION_SEQ</param>
							</generator>
		</id>
		<property
			name="name"
			column="RELIGION_VALUE"
			
		/>
		<property
							name="fromDate"
							column="START_DATE"
							
				/>
				<property
							name="toDate"
							column="END_DATE"
							
		/>
	</class>
</hibernate-mapping>
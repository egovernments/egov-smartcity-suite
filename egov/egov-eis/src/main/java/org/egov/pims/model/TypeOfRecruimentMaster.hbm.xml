<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.pims.model.TypeOfRecruimentMaster" table="EGEIS_RECRUITMENT_TYPE_MSTR">
		
		<id
							name="id"
							column="RECRUITMENT_TYPE_ID"
							type="int">
							<generator class="sequence">
								<param name="sequence_name">EGPIMS_RECRUITMENT_TYPE_SEQ</param>
							</generator>
		</id>
		<property
			name="name"
			column="RECRUITMENT_TYPE_NAME"
			
		/>
		<property
							name="fromDate"
							column="START_DATE"
							
				/>
				<property
							name="toDate"
							column="END_DATE"
							
		/>
		
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.egov.lib.admbndry.CrossHeirarchyImpl" table="EG_CROSSHEIRARCHY_LINKAGE">
		<id name="id" column="ID" type="int">
			<generator class="native">
				<param name="sequence">SEQ_EG_CROSSHEIRARCHY_LINKAGE</param>
			</generator>
		</id>
		<many-to-one name="parent" class="org.egov.lib.admbndry.BoundaryImpl" column="PARENT" />
		<many-to-one name="child" class="org.egov.lib.admbndry.BoundaryImpl" column="CHILD" />
	</class>
</hibernate-mapping>